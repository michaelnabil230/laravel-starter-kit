{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "michaelnabil230/laravel-starter-kit",
    "type": "project",
    "description": "An opinionated starter kit for building Laravel applications.",
    "keywords": [
        "laravel",
        "framework",
        "starter-kit"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "inertiajs/inertia-laravel": "^2.0.10",
        "laravel-notification-channels/fcm": "^5.1.0",
        "laravel/framework": "^12.35.1",
        "laravel/horizon": "^5.37.0",
        "laravel/pulse": "^1.4.3",
        "laravel/telescope": "^5.14.1",
        "laravel/tinker": "^2.10.1",
        "maatwebsite/excel": "^3.1.67",
        "mcamara/laravel-localization": "^2.3.0",
        "pinkary-project/type-guard": "^0.1.0",
        "predis/predis": "^3.2.0",
        "propaganistas/laravel-phone": "^6.0.2",
        "sentry/sentry-laravel": "^4.18.0",
        "spatie/cpu-load-health-check": "^1.0.4",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-backup": "^9.3.5",
        "spatie/laravel-health": "^1.34.8",
        "spatie/laravel-login-link": "^1.6.3",
        "spatie/laravel-medialibrary": "^11.17.0",
        "spatie/laravel-query-builder": "^6.3.6",
        "spatie/laravel-schedule-monitor": "^4.1.0",
        "spatie/laravel-searchable": "^1.13.0",
        "spatie/laravel-translatable": "^6.11.4",
        "spatie/security-advisories-health-check": "^1.3.1",
        "tightenco/ziggy": "^2.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16.0",
        "driftingly/rector-laravel": "^2.1.1",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.7.2",
        "laravel/boost": "^1.4.0",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.25.1",
        "laravel/sail": "^1.46.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "pestphp/pest": "^4.1.2",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "pestphp/pest-plugin-type-coverage": "^4.0.2",
        "rector/rector": "^2.2.5",
        "soloterm/solo": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan clear",
            "@update:requirements"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan app:install --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ],
        "update:requirements": [
            "composer bump",
            "npx npm-check-updates -u"
        ],
        "format": "pint --parallel",
        "refactor": "rector",
        "test:type-coverage": "pest --type-coverage --min=99.6 --memory-limit=2G",
        "test:format": "pint --parallel --test",
        "test:unit": "pest --parallel --coverage --exactly=38.6",
        "test:types": "phpstan --memory-limit=2G",
        "test:refactor": "rector --dry-run",
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test --parallel",
            "@test:type-coverage",
            "@test:unit",
            "@test:format",
            "@test:types"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
